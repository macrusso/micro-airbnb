<% include ../partials/header.ejs %>

<h1>index of places</h1>

<div class="row">
    <% places.forEach(function (place) { %>
        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <img src="<%= place.photo %>">
                <div class="caption">
                    <h3><%= place.name %></h3>
                    <p><%= place.info %></p>
                    <p><a href="#" class="btn btn-primary" role="button">More</a></p>
                </div>
            </div>
        </div>
    <% })%>
</div>



<% include ../partials/footer.ejs %>